setInterval(function() {

  var btnAposta = document.querySelector(".btnBet");
  var btns = document.getElementsByClassName("amount__btn");
  var btnMetade = btns[2];
  var btns = document.getElementsByClassName("amount__btn");
  var btnDuplicar = btns[3];


  btnMetade.click();
  btnDuplicar.click();
  btnAposta.click();
  
}, 5000);